int main(int argc,char* argv[])
{
    CvCapture *capture;
    IplImage *frame;
    capture=cvCreateCameraCapture(0);
    cvNamedWindow("Webcam",1);

    PrintMatrix(m);
    
    while(true)
    {
        frame = cvQueryFrame(capture);
        if(!frame)break;
        cvShowImage("Webcam",frame);
        if(cvWaitKey(20)>0)
            break;
    }
    cvReleaseCapture(&capture);
    cvDestroyWindow("Webcam");
    return 0;
